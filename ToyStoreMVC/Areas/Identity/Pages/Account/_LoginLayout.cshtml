<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>@ViewData["Title"] - Teddy Store</title>
	<meta content="width=device-width, initial-scale=1.0" name="viewport">
	<meta content="" name="keywords">
	<meta content="" name="description">

	<!-- Google Web Fonts -->
	<link rel="preconnect" href="https://fonts.googleapis.com">
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
	<link href="https://fonts.googleapis.com/css2?family=Fredoka:wght@600;700&family=Montserrat:wght@200;400;600&display=swap" rel="stylesheet">

	<!-- Icon Font Stylesheet -->
	<link rel="shortcut icon" type="image/x-icon" href="https://teddy.vn/wp-content/uploads/2022/08/Teddy-2022.png" />
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" />
	<link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

	<!-- Libraries Stylesheet -->
	<link href="~/assets/animate/animate.min.css" rel="stylesheet">
	<link href="~/assets/lightbox/css/lightbox.min.css" rel="stylesheet">
	<link href="~/assets/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

	<!-- Customized Bootstrap Stylesheet -->
	<link href="~/css/bootstrap.min.css" rel="stylesheet">

	<!-- Template Stylesheet -->
	<link href="~/css/style.css" rel="stylesheet">
</head>
<body>
	<header class="container-fluid px-0">
		<nav class="navbar navbar-expand-xl px-5 pb-1 pt-2" style="box-shadow: 0px 15px 10px -15px">
			<a class="navbar-brand col-lg-2 me-5" asp-area="Customer" asp-controller="Home" asp-action="Index"><h1 class="text-primary display-6">Teddy<span class="text-secondary">Store</span></h1></a>
			<h4 class="text-primary">@ViewData["Title"]</h4>
		</nav>
	</header>
	<main>
		@RenderBody()
	</main>
	<!-- JavaScript Libraries -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/assets/wow/wow.min.js"></script>
	<script src="~/assets/easing/easing.min.js"></script>
	<script src="~/assets/waypoints/waypoints.min.js"></script>
	<script src="~/assets/lightbox/js/lightbox.min.js"></script>
	<script src="~/assets/owlcarousel/owl.carousel.min.js"></script>

	<!-- Template Javascript -->
	<script src="~/js/main.js"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>

</html>